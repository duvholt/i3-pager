<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
	<kcfgfile name="" />
	<group name="General">
		<entry name="showWorkspaceNames" type="Bool">
			<default>
				true
			</default>
		</entry>
		<entry name="filterByCurrentScreen" type="Bool">
			<default>
				true
			</default>
		</entry>
		<entry name="urgentColorWholeWorkspace" type="Bool">
			<default>
				true
			</default>
		</entry>
		<entry name="orderWorkspacesBy" type="String">
			<default>
				default
			</default>
		</entry>
		<entry name="rounded" type="Bool">
			<default>
				true
			</default>
		</entry>
		<entry name="style" type="String">
			<default>
				line
			</default>
		</entry>
		<entry name="indicatorPosition" type="String">
			<default>
				bottom
			</default>
		</entry>
	</group>
	<group name="Screens">
		<entry name="colorWorkspaceByScreen" type="Bool">
			<default>
				false
			</default>
		</entry>
		<entry name="screenNameList" type="StringList">
		</entry>
		<entry name="screenColorList" type="StringList">
		</entry>
		<entry name="screenColorOpacity" type="Double">
			<default>
				0.5
			</default>
		</entry>
	</group>
</kcfg>
